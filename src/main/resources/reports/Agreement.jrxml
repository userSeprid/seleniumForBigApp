	<queryString>
		<![CDATA[SELECT
  (SELECT title from months_ukr where number = EXTRACT(MONTH FROM now()) ) AS month_word_ukr,
  contract_number(c.id) as contract_number,
/*
  p.*,
  c.*,
*/
  cs.refund,
  cs.payment,
  p.reaction_time,
  (select name from person ps, ppk_person pp where ps.id = pp.person_id and pp.ppk_id = p.id limit 1) as person_name,
  p.address,
  (select number_to_string(payment::INTEGER) as string_payment),
  (select number_to_string(cs.refund::INTEGER) as string_responsibility),
  (select title from dictionaries where id = p.object_type_id) as object,
  (select scheme from ppk_files where ppk_id = p.id) as scheme,
  (select map from ppk_files where ppk_id = p.id) as map_,
  (select map_details from ppk_files where ppk_id = p.id) as map_details
FROM
  ppk p, contract c, contract_state cs
WHERE
  p.id = c.ppk_id and c.id = cs.contract_id
AND
  cs.id = $P{contractStateId}]]>
	</queryString>
